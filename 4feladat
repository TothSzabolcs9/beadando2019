import datetime

y=datetime.datetime(2018,12,7)
x=datetime.datetime(2018,12,6)
x1=x.strftime("%j")
y1=y.strftime("%j")
a=int(x1)-int(y1)
if(a>0):
    print(a)
else:
    y = datetime.datetime(2018, 12, 7)
    x = datetime.datetime(2018, 12, 31)
    x1 = x.strftime("%j")
    y1 = y.strftime("%j")
    a = int(x1) - int(y1)
    y = datetime.datetime(2019, 1, 1)
    x = datetime.datetime(2018, 12, 6)
    x1 = x.strftime("%j")
    y1 = y.strftime("%j")
    b = int(x1) - int(y1)
    print(a+b)
